  "authors": [
    "GSimken",
    "InTeractiOn"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/Gsimken/BGames-Minecraft-Library-Forge-Fabric/"
  },

  "license": "GPLv3",
  "icon": "interaction_logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.gsimken.bgameslibrary.BgamesLibrary"
    ],
    "client": [
      "net.gsimken.bgameslibrary.BgamesLibraryClient"
    ]
  },
  "mixins": [
    "bgameslibrary.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
